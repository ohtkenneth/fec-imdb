{
  "name": "fec-imdb",
  "version": "0.0.1",
  "description": "Front end implementation of imdb",
  "main": "./src/server/server.js",
  "scripts": {
    "test": "jest",
    "react:dev": "webpack --mode development --watch",
    "server:dev": "webpack-dev-server --open --mode development",
    "build:dev": "webpack --mode development --config webpack.dev.js --watch",
    "build:prod": "webpack --mode production --config webpack.prod.js",
    "start": "node ./src/server/server.js",
    "load": "node ./src/db/utils/loadData.js",
    "nodemon": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ohtkenneth/fec-imdb.git"
  },
  "author": "Kenneth Oh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ohtkenneth/fec-imdb/issues"
  },
  "homepage": "https://github.com/ohtkenneth/fec-imdb#readme",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "body-parser": "^1.18.3",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "helmet": "^3.13.0",
    "mongoose": "^5.2.13",
    "morgan": "^1.9.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-test-renderer": "^16.5.0",
    "regenerator-runtime": "^0.12.1",
    "style-loader": "^0.23.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-combine-loaders": "^2.0.4"
  },
  "devDependencies": {
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.0",
    "eslint-plugin-react": "^7.11.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "sinon": "^6.3.1",
    "superagent": "^3.8.3",
    "supertest": "^3.3.0",
    "webpack-dev-server": "^3.1.7"
  },
  "proxy": "http://localhost:1337",
  "jest": {
    "setupTestFrameworkScriptFile": "./setupTests.js",
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/mockdata/**",
      "!**/styles/**",
      "!**/dist/**"
    ],
    "coverageThreshold": {
      "./src/client/Components": {
        "statements": 65
      },
      "./src/db/": {
        "statements": 65
      }
    }
  }
}
